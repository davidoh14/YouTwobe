<!DOCTYPE html>
<html>
  <head>
    <title>YouTwobe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap"
    />

    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
    />

    <%= favicon_link_tag asset_path('YouTube-Icon.svg')%> 

    <script type="text/javascript">
      window.YouTubeIconURL = "<%= image_url('YouTube-Icon.png') %>";
      window.potionsellerPURL = "<%= image_url('potionsellerP.png') %>";
      window.rainPURL = "<%= image_url('rainP.png') %>";
      window.selfdefensePURL = "<%= image_url('selfdefenseP.png') %>";
      window.squirrel_thumbnailPURL = "<%= image_url('squirrel_thumbnail.png') %>";
      window.spidermanPURL = "<%= image_url('spidermanP.png') %>";
      window.unacceptablePURL = "<%= image_url('unacceptableP.png') %>";
    </script>
  
    <script type="text/javascript">
      window.potionsellerURL = "<%= image_url('potionseller.mp4') %>";
      window.rainURL = "<%= image_url('rain.mp4') %>";
      window.selfdefenseURL = "<%= image_url('selfdefense.mp4') %>";
      window.squirrelURL = "<%= image_url('squirrel240.mp4') %>";
      window.spidermanURL = "<%= image_url('spiderman.mp4') %>";
      window.unacceptableURL = "<%= image_url('unacceptable.mp4') %>";
    </script>


    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
